{
  "extends": "airbnb",
  "rules": {
    "comma-dangle": ["error", "never"],
    "jsx-quotes": ["error", "prefer-single"],
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
            "no-confusing-arrow": ["error", {"allowParens": true}]
  },

  "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8,
      "ecmaFeatures": {
          "jsx": true,
          "experimentalObjectRestSpread": true
      }
  }
}
